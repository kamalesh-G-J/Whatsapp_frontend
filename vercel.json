{
  "version": 2,
  "builds": [
    {
      "src": "frontend_deploy/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "frontend_deploy/build/index.html" }
  ]
}
